    <name value="HL7 AU Connectathon - Provider Directory - PractitionerRole Locate Identifier - XML"/>  
    <status value="draft"/>  
    <date value="2017-07-18"/>  
    <publisher value="HL7 AU"/>  
    <contact> 
        <name value="HL7 AU"/>  
        <telecom> 
            <system value="email"/>  
            <value value="brett.esler@oridashi.com.au"/>  
            <use value="work"/> 
