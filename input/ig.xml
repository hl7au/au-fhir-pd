      <exampleCanonical value="http://hl7.org.au/fhir/pd/StructureDefinition/au-pd-practitioner"/>
    </resource>

    <resource>
      <reference>
        <reference value="PractitionerRole/example0"/>
      </reference>
      <name value="PractitionerRole - Dr Helen Mayo"/>
